{
  "name": "access-select",
  "version": "0.1.0",
  "description": "",
  "main": "es2015/src/index.js",
  "gcloud": {
    "id": "gweb-creatability",
    "bucket": "acc-components",
    "version": "1",
    "devPort": 8080,
    "devAdminPort": 8000
  },
  "scripts": {
    "build": "webpack --mode=production --reporter basic",
    "build-ua5": "npm run build; cp dist/* ../acc-creativity-clarion/third_party/; cp dist/* ../acc-creativity-keyboard/third_party/; cp dist/* ../acc-creativity-sampler/third_party; cp dist/* ../acc-creativity-bodysynth/third_party",
    "doc": "rm -rf ./doc && typedoc --excludeExternals --excludePrivate --ignoreCompilerErrors --includeDeclarations --exclude \"dist\/*\"  --theme \"minimal\" --out ./doc",
    "tsc": "tsc",
    "watch-copy": "export COMPONENTS_BUNDLE_DESTINATION=../examples/assets",
    "dev": "concurrently \"node watch-copy.js\" \"webpack --watch --reporter basic --mode=development\" \"http-server\"",
    "dev-kyle": "npm run watch-copy && npm run dev",
    "start": "npm run dev",
    "set-cors": "gsutil cors set cors.json gs://$npm_package_gcloud_bucket",
    "make-bucket-public": "gsutil -m acl ch -u AllUsers:R gs://$npm_package_gcloud_bucket/**/*",
    "upload-stencil-bucket": "gsutil -m cp -r ./dist/** gs://$npm_package_gcloud_bucket",
    "update-bucket": "npm run upload-stencil-bucket && npm run make-bucket-public"
  },
  "author": "Google Creative Lab",
  "license": "Apache-2.0",
  "dependencies": {
    "@polymer/lit-element": "0.5.2",
    "@polymer/polymer": "3.0.5",
    "@tensorflow-models/posenet": "^0.2.2",
    "@tensorflow/tfjs": "^0.13.0",
    "@types/webassembly-js-api": "0.0.1",
    "@webcomponents/webcomponentsjs": "2.0.0",
    "animitter": "3.0.0",
    "assert": "1.4.1",
    "eventemitter": "0.3.3",
    "http-server": "0.11.1",
    "lit-html": "^0.10.0",
    "raw-loader": "^0.5.1",
    "unmute": "^0.1.3",
    "webmidi": "^2.2.0"
  },
  "devDependencies": {
    "@webpack-cli/serve": "0.0.8",
    "autobind-decorator": "^2.1.0",
    "babel-core": "6.26.0",
    "babel-preset-env": "1.6.1",
    "chalk": "2.4.1",
    "concurrently": "3.6.0",
    "copy-webpack-plugin": "4.5.1",
    "is-iexplorer": "^1.0.0",
    "shortid": "2.2.8",
    "text-loader": "0.0.1",
    "ts-loader": "4.4.2",
    "typedoc": "^0.13.0",
    "typescript": "2.9.2",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "4.12.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "3.0.8",
    "webpack-command": "0.2.1",
    "webpack-dev-server": "3.1.4"
  }
}
